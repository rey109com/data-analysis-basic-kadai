      "source": [
        "import numpy as np\n",
        "\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "C = np.dot(A, B)\n",
        "\n",
        "print(\"行列の積：\")\n",
        "print(C)\n",
        "\n",
        "max_value = np.max(C)\n",
        "print(\"計算された行列の最大値：\", max_value)\n"
      ]
    }
